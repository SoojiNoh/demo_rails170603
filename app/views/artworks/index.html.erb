<table>
  <tr>
    <th>Title</th>
    <th>Artist Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
<% @artworks.each do |artwork| %>
  <tr>
    <td><%= artwork.title %></td>
    <td><%= artwork.size %></td>
    <td><%= link_to "Show", artwork %></td>
    <td><%= link_to "Edit", edit_artwork_path(artwork) %></td>
    <td><%= link_to "Remove", artwork, method: :delete, data: { confirm: "Are you sure?" } %></td>
  </tr>
<% end %>
</table>
 
