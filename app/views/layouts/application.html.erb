  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  
  <title>| Dashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%#= custom_bootstrap_flash %>
  <%= csrf_meta_tags %>
  
  <!-- For example, <link href="<#%= asset_path "materialize.css" %>" type="text/css" rel="stylesheet" media="screen,projection"data-turbolinks-track="reload">-->

  <link href="<%= asset_path "bootstrap/css/bootstrap.css" %>" type="text/css" rel="stylesheet" data-tubolinks-tracks="reload">
  <link href="<%= asset_path "font-awesome/css/font-awesome.css" %>" type="text/css" rel="stylesheet" data-turbolinks-tracks="reload">
  <link href="<%= asset_path "animate.css"%>" type="text/css" rel="stylesheet" data-turbolinks-tracks="reload" >
