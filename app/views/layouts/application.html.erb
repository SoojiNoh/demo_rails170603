  <link href="<%= asset_path "custom.css"%>" type="text/css" rel="stylesheet" data-turbolinks-tracks="reload" >

  <link href="<%= asset_path "animate.css"%>" type="text/css" rel="stylesheet" data-turbolinks-tracks="reload" >

  <!-- Toastr style -->
  <link href="<%= asset_path "plugins/toastr/toastr.min.css"%>" type="text/css" rel="stylesheet" data-turbolinks-tracks="reload" >
    
  <!-- Latest compiled and minified CSS -->
  <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">-->
  
  <!-- Optional theme -->
  <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">-->
  
  <!-- Latest compiled and minified JavaScript -->
  <!--<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>-->



</head>
<body 
  <% if content_for?(:body_attributes) %>
    <%= yield(:body_attributes) %> 
  <% end %>>

<div id="preloader"></div>  
<div id="wrapper">
  <%= render partial: "layouts/dashboard_sidenav" %>

    <div id="page-wrapper" class="gray-bg">
        <div class="row border-bottom">
          <%= render partial: "layouts/dashboard_topnav" %>
        </div>
        <% flash.each do |name, msg| %>
          <% if msg.class == Array %>
            <% msg.each do |message| %>
                <%javascript_tag do %>
                  window.onload = function() {
                    setTimeout(function() {
                      toastr.options = {
                      closeButton: true,
                      progressBar: true,
                      showMethod: 'slideDown',
                      timeOut: 4000
                    };
                      toastr.success('<%=message%>');
                    }, 1300);
                  }
                <%end%>
              <%#= content_tag :div, message, :id => "flash_#{name}" %>
            <% end %>
          <% else %>
            <%=javascript_tag do %>
              window.onload = function() {
                setTimeout(function() {
                  toastr.options = {
                  closeButton: true,
                  progressBar: true,
                  showMethod: 'slideDown',
                  timeOut: 4000
                };
                  toastr.success('<%=msg%>');
                }, 1300);
              }
            <%end%>
          <%#= content_tag :div, msg, :id => "flash_#{name}" %>
          <% end %>
        <% end %>
        <%=yield %>
    </div>
  
</div>

<!-- Mainly scripts -->

<!--For example,     <script type="text/javascript" src="<%#= asset_path "prism.js" %>"></script> -->

<script type="text/javascript" src="<%= asset_path "bootstrap.min.js" %>"></script>
<script type="text/javascript" src="<%= asset_path "plugins/metisMenu/jquery.metisMenu.js" %>"></script>
<script type="text/javascript" src="<%= asset_path "plugins/slimscroll/jquery.slimscroll.min.js" %>"></script>

<!-- Custom and plugin javascript -->
<script type="text/javascript" src="<%= asset_path "inspinia.js" %>"></script>
<script type="text/javascript" src="<%= asset_path "plugins/pace/pace.min.js" %>"></script>

<!-- jQuery UI -->
<script type="text/javascript" src="<%= asset_path "plugins/jquery-ui/jquery-ui.min.js" %>"></script>
<script type="text/javascript" src="<%#= asset_path "jquery-3.1.1.min.js" %>"></script>

<!-- Toastr -->
<script type="text/javascript" src="<%= asset_path "plugins/toastr/toastr.min.js" %>"></script>


